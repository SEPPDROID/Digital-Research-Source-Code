$nolist
/*
    Th Externals
*/

  attach:
    procedure (pdadr) external;
      declare pdadr address;
    end attach;

  detach:
    procedure (pdadr) external;
      declare pdadr address;
    end detach;

  assign:
    procedure (pname) byte external;
      declare pname address;
    end assign;

$list
