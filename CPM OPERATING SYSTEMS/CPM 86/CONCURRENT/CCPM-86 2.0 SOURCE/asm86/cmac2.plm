        /* IRET */
iret1   cmac3struc public data(
        nil,0,
        mdbn,0cfh,          /* DB 0CFH */
        mendm),             /* ENDM */

        /* JA place:Cb */
ja1     cmac7struc public data(
        nil,1,
        specC,modb,
        mdbn,77h,           /* DB 77H */
        mrelb,place,        /* RELB place */
        mendm),             /* ENDM */

        /* JAE place:Cb */
jae1    cmac7struc public data(
