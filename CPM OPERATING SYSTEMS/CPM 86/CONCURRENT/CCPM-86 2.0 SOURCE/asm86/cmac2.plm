        mdbn,0c4h,          /* DB 0C4H */
        mmodrm2,dst,src,    /* MODRM dst,src */
        mendm),             /* ENDM */

        /* LEA dst:Rw,src:M */
lea1    cmac10struc public data(
        nil,2,
        specR,modw,specM,nomod,
        mdbn,8dh,           /* DB 8DH */
        mmodrm2,dst,src,    /* MODRM dst,src */
        mendm),             /* ENDM */

        /* LOCK prefx */
lock1   cmac3struc public data(
        nil,0+prefix$on,
        mdbn,0f0h,          /* DB 0F0H */
        mendm),             /* ENDM */

