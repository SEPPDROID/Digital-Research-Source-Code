        mdbn,9fh,           /* DB 9FH */
        mendm),             /* ENDM */

        /* LDS dst:Rw,src:Ed */
lds1    cmac12struc public data(
        nil,2,
        specR,modw,specE,modd,
        msegfix,src,        /* SEGFIX src */
        mdbn,0c5h,          /* DB 0C5H */
        mmodrm2,dst,src,    /* MODRM dst,src */
        mendm),             /* ENDM */

        /* LES dst:Rw,src:Ed */
les1    cmac12struc public data(
        nil,2,
