inc1    cmac10struc data(
        nil,1,
        specE,modb,
        msegfix,dst,         /* SEGFIX dst */
        mdbn,0feh,           /* DB 0FEH */
        mmodrm1,0,dst,       /* MODRM 0,dst */
        mendm),              /* ENDM */

        /* INC dst:Ew */
inc2    cmac10struc data(
        .inc1,1,
        specE,modw,
        msegfix,dst,         /* SEGFIX dst */
