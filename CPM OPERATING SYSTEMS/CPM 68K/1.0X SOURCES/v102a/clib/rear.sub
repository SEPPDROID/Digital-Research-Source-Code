;;;;;;;
;;;;;;;

;;;;;;;
; SUBMIT R3
;;;;;;;
; STEP 3: ARCHIVE ASSEMBLER STUFF

$2AR68 RF $1 NEWLIB   ABORT.O ALDIV.O ALMUL.O ALREM.O LDIV.O LMUL.O LREM.O
$2AR68 RF $1 NEWLIB   SALLOC.O SETJMP.O ULDIV.O SIGNAL.O XSIGNAL.O BLIVOT.O
;;;;;;;
;;;;;;;

ERA CLIB
REN CLIB=NEWLIB
PIP $2[G0]=CLIB
PIP $1[G0]=S.O
PIP $2[G0]=S.O
REN S.X=S.O
ERA *.O
REN S.O=S.X
USER 7!MAKE $1 $2
